{
  "name": "next-with-apollo-hoc",
  "version": "1.0.0",
  "main": "lib",
  "module": "esm",
  "typings": "types",
  "license": "MIT",
  "repository": "pawsong/next-with-apollo-hoc",
  "files": [
    "esm",
    "lib",
    "src",
    "types"
  ],
  "scripts": {
    "clean": "rimraf esm lib types",
    "build": "yarn build:types && yarn build:browser && yarn build:node",
    "build:types": "tsc --project tsconfig.types.json",
    "build:js": "babel src --extensions \".ts,.tsx\" --source-maps true",
    "build:browser": "BROWSER=true yarn build:js --out-dir esm",
    "build:node": "BROWSER=false yarn build:js --out-dir lib",
    "dev": "DEMO=true next",
    "prepublishOnly": "yarn clean && yarn build"
  },
  "dependencies": {
    "@apollo/react-common": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@babel/runtime": "^7.9.2",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "next-with-apollo": "^5.0.0"
  },
  "peerDependencies": {
    "next": "^9",
    "react": "^16"
  },
  "devDependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "@babel/preset-typescript": "^7.9.0",
    "@types/node": "^13.9.2",
    "@types/react": "^16.9.25",
    "apollo-server-micro": "^2.11.0",
    "babel-plugin-graphql-tag": "^2.5.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "next": "^9.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^3.8.3"
  }
}
